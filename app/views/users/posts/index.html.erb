<p id="notice"><%= notice %></p>

<h1>Users Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.content %></td>
        <td><%= link_to 'Show', users_post_path(id: post.id) %></td>
        <td><%= link_to 'Edit', edit_users_post_path(id: post.id) %></td>
        <td><%= link_to 'Destroy', users_post_path(id: post.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users Post', new_users_post_path %>
